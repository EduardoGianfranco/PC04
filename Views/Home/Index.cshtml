@model List<Fail>
@{
    ViewData["Title"] = "Home Page";
}

@if(Model.Count() == 0) {
    <h1>Aún no se han registrado fotos</h1>
}else{
    <div class="row">
        @foreach (var fail in Model)
        {
            <div>
                <h2>Título: @fail.Titulo</h2>
                <img src="@fail.Foto">
                <h5>@fail.Usuario</h5>
            </div>
        }
    </div>
}
